<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <flow Re="10000" />
    <simulation finalTime="100.0" >
        <type>turbulence-sa</type>
        <scenario>channel</scenario>
    </simulation>
    <turbulent kappa = "0.41">
        <boundaryLayerType>turbulence</boundaryLayerType>
    </turbulent>
    <timestep dt="1" tau="0.5" />
    <solver gamma="0.5" />
    <geometry
      dim="2"
      lengthX="30.0" lengthY="1.0" lengthZ="1.0"
      sizeX="100" sizeY="150" sizeZ="10"
      stretchX="false" stretchY="false" stretchZ="false"
    >
      <mesh>uniform</mesh> 
      <!--mesh>stretched</mesh-->
    </geometry>
    <environment gx="0" gy="0" gz="0" />
    <walls>
        <left>
            <vector x="1.0" y="0" z="0" />
        </left>
        <right>
            <vector x="0" y="0" z="0" />
        </right>
        <top>
            <vector x="0.0" y="0." z="0." />
        </top>
        <bottom>
            <vector x="0" y="0" z="0" />
        </bottom>
        <front>
            <vector x="0" y="0" z="0" />
        </front>
        <back>
            <vector x="0" y="0" z="0" />
        </back>
    </walls>
    <vtk interval="0.1">Channel2D</vtk>
    <stdOut interval="0.0001" />
    <parallel numProcessorsX="1" numProcessorsY="1" numProcessorsZ="1" />
</configuration>
